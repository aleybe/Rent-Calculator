{
    "manifest_version": 2,
    "name": "Rent Costs Analysis",
    "description": "This extension will tell you if you can afford rent or not",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.4.1.min.js",
                "script.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "declarativeContent",
        "storage",
        "activeTab",
        "tabs"
    ]
}